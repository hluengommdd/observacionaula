VITE_SUPABASE_URL=
# Client-side key (public anon key). Prefer to use a more limited key for client.
VITE_SUPABASE_KEY=

# Optional: URL or path to logo used by the app (e.g. /logo-oficial.png)
VITE_LOGO_URL=/logo-oficial.png

# Server-side (keep secret, set these only in server/hosting environment):
# SUPABASE_URL should match VITE_SUPABASE_URL but with service role usage on server
SUPABASE_URL=
SUPABASE_KEY=

# Server port for local server (server.js)
PORT=3001
